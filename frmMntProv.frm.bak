Private Sub Form_Activate()
 
 If Not prime Then
 
 If rc.RecordCount = 0 Then
        
        If MsgBox("No se encuentran Proveedores. ¿Crear?", vbYesNo + vbQuestion, "Secciones") = vbNo Then
        Unload Me
        Else
        Call cbAgregar_Click
        End If
 Else
    
        Call cmdFirst_Click
 End If
    
 prime = True
 End If
    
End Sub

Private Sub Form_Load()
  
    With locCnn
    If .State = 0 Then
        .CursorLocation = adUseClient
        .Open strLocCnn
    End If
   End With
  
  Set rc = New Recordset
  
  oSQL.AddTable "MAPROV"
  oSQL.AddOrderClause "CODIGO"
