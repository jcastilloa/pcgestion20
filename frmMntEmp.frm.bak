Private Sub ioCIF_Validate(Cancel As Boolean)
If Trim(ioCIF.Text) <> "" Then Call comprueba_DNI(Trim(ioCIF.Text), ioCIF)
End Sub



Private Sub Form_Activate()
  
  If Not prime Then
  If rc.RecordCount = 0 Then
        
        If MsgBox("No se encuentran Empresas. ¿Crear?", vbYesNo + vbQuestion, "Secciones") = vbNo Then
        Unload Me
        Else
        Call cbAgregar_Click
        End If
        
  Else
        Call cmdFirst_Click
        
  End If
  prime = True
  End If
    
End Sub

Private Sub Form_Load()
  
  Set EmpCnn = New ADODB.Connection
  
